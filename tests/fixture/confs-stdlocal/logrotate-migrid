/home/testuser/migrid/state/log/*.log /home/testuser/migrid/state/log/*.out {
	weekly
	missingok
	rotate 52
	compress
	delaycompress
	notifempty
	create 640 testuser testgroup
	# NOTE we need root to restart services
	#su testuser testgroup
	su root testgroup
	sharedscripts
	postrotate
           service migrid reload; \
           service apache2 reload;
	endscript
}
